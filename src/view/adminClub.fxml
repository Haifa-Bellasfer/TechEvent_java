<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminClubController">
   <children>
      <VBox prefHeight="600.0" prefWidth="250.0" style="-fx-background-color: #214a48;">
         <children>
            <Label alignment="CENTER" prefHeight="100.0" prefWidth="250.0" text="Admin Dashborad" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font name="Cambria Bold" size="24.0" />
               </font>
            </Label>
            <Label fx:id="label_news" alignment="CENTER" prefHeight="37.0" prefWidth="255.0" text="News" textAlignment="CENTER" textFill="#dad5d5">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Label>
            <Label fx:id="label_news1" alignment="CENTER" prefHeight="37.0" prefWidth="255.0" text="Clubs" textAlignment="CENTER" textFill="#dad5d5">
               <font>
                  <Font name="Cambria" size="18.0" />
               </font>
            </Label>

         </children>
      </VBox>
      <VBox layoutX="250.0" prefHeight="600.0" prefWidth="750.0" style="-fx-background-color: #D9E3DA;" >
          <children>
      <TableView fx:id="clubs" prefHeight="602.0" prefWidth="750.0" onMouseClicked="#popShow">
        <columns>
          <TableColumn fx:id="name" prefWidth="117.0" text="Club name" />
          <TableColumn fx:id="desc" prefWidth="179.0" text="Description" />
            <TableColumn fx:id="mail" prefWidth="140.0" text="Email" />
            <TableColumn fx:id="st" prefWidth="163.0" text="Status" />
        </columns>
      </TableView>
          </children>
      </VBox>
   </children>
</AnchorPane>